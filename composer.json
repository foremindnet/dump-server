{
  "name": "foremind/dump-server",
  "description": "Development tool: Enhanced Laravel dump server with custom functionality (install with --dev)",
  "version": "1.0.0",
  "type": "library",
  "minimum-stability": "stable",
  "license": "proprietary",
  "suggest": {
    "Install with --dev flag": "This package is intended for development use only. Install with: composer require foremind/dump-server --dev"
  },
  "keywords": ["laravel", "debugging", "development", "dump", "var-dump", "dev-only"],
  "authors": [
    {
      "name": "Cam Kemshal-Bell",
      "email": "cam@foremind.com.au"
    }
  ],
  "require": {
    "php": "^8.3",
    "laravel/framework": "^12.0",
    "symfony/var-dumper": "^7.0"
  },
  "require-dev": {
    "laravel/pint": "^v1"
  },
  "autoload": {
    "psr-4": {
      "Foremind\\DumpServer\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Foremind\\DumpServer\\DumpServerServiceProvider"
      ]
    }
  },
  "prefer-stable": true
}